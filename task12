#include<iostream>
#include<stack>
using namespace std;
const int N = 1e3 + 3;
int a[N];
int main(){
    freopen("hihi.inp","r", stdin);
    freopen("hihi.out","w",stdout);
    int n;
    while( cin >> n && n != 0){
        while(true){ // vong lap vo han(phai co dieu kien)
            cin >> a[1];
            if(a[1] == 0){
                cout << endl;
                break;
            }
            for( int i = 2; i <= n; i++){
                cin >> a[i];
            }
            int j=1;
            stack<int> st;
            for(int i = 1; i <= n; i++){
                st.push(j);
                while( !st.empty() && st.top() == a[i]){
                    st.pop();
                    j++;
                }
            }
            if( st.empty()) cout << "Yes" << endl;
            else cout << "No" << endl;
        }
    }
    return 0;
}
