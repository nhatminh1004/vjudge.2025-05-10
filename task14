#include<iostream>
#include<stack>
int compare(char c){
    if(c=='+'||c=='-')return 1;
    else if(c=='*'||c=='/')return 2;
    return 0;
}
using namespace std;
int main(){
    int n;
    cin>>n;
    cin.ignore();
    while(n--){
        string s;
        getline(cin,s);
        stack<char>st;
        for(char c:s){
            if('0'<=c&&c<='9')cout<<c;
            else if(c=='(')st.push(c);
                else if(c==')'){
                    while( !st.empty()&&st.top()!='('){
                        cout<<st.top();
                        st.pop();
                    }
                    if( !st.empty())st.pop();//xoa (
                }
                    else{
                        while(!st.empty()&&( compare(st.top())>=compare(c))){
                            cout<<st.top();
                            st.pop();
                        }
                        st.push(c);
                    }
        }
        while( !st.empty()){
            cout<<st.top();
            st.pop();
        }
        cout<<endl;
    }

    return 0;
}
